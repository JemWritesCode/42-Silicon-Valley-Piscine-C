NAME = ft_cat
SRCS = ft_cat.c

all: 
	gcc -Wall -Wextra -Werror $(SRCS) -o $(NAME)

clean:
	/bin/rm -f *.o

fclean: clean
	/bin/rm -f $(NAME)
